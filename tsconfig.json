{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "importHelpers": false,
    "noEmitHelpers": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "node_modules",
    "tmp",
    "dist",
    "bazel-out"
  ]
}
